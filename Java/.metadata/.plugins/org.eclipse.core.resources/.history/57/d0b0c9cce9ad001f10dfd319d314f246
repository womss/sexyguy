import java.util.Iterator;

public class RMain1 {
	public static void main(String[] args) {
		// for, while, do-while
		// 반복문 10번 => 반복문 1번 돌때마다
		//	안녕하세요 x 3번씩 출력
		// => 갑자기 숫자 1이 맘에 안들어서
		for (int i = 1; i <=10; i++) {
			if (i==1) {
				continue;
			}
			System.out.printf("========%d번째=========\n", i);
			for (int j = 1; j <=3; j++) {
				
				System.out.println("안녕하세요");
//			}System.out.printf("========%d번째=========\n", i);
		}
		System.out.println("===========================");
		 
		
		
	}
		for (int i = 0; i < 10; i++) {
			if (i % 2 == 0) {
				System.out.println("와");
			}else {
				continue;	// 강제 턴 종료
							// 홀수일때는 무시하고, 다음으로 넘어감
			}
		}
}
}
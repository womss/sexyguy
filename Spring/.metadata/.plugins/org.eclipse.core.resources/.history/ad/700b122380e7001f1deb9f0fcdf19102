 	<bean id="d1" class="com.beaver.feb102.dog.Dog">
 		<property name="name" value="멈무" />
 		<property name="age" value="5" />
 	</bean>
 	<bean id="c1" class="com.beaver.feb102.cat.Cat">
 		<property name="name" value="먀몸" />
 		<property name="age" value="2" />
 	</bean>
 	
 	<!--
 		어딘가에 bean을 등록해놓으면

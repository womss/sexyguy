	<bean id="h1" class="com.beaver.feb102.human.Human">
		<property name="name" value="김뜨또" />
		<property name="age" value="200" />
		<property name="nickname">
			<list value-type="java.lang.String">
				<value>비버</value>
				<value>돼지</value>
				<value>저스트뜨또</value>

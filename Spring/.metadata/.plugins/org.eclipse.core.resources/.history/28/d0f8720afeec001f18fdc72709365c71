	<select id="getAllFruit" resultType="com.beaver.feb171.fruit.Fruit"> <!-- 셀렉문을 눌렀을때의 결과가 과일객체가 나오게 됨 -->
		select * from feb17_fruit order by f_price
	</select>
	<select id="getFruitPrice" resultType="com.beaver.feb171.fruit.Fruit">
    select f_name, f_price FROM feb17_fruit WHERE f_price <= #{price}
	</select>
	

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript" src="jQuery.js"></script>
<script type="text/javascript">
	$(function() {
		// ㅋㅋㅋㅋㅋ => 글자색 : green / 배경색 : red
		// ㅎㅎㅎㅎㅎ => 글자색 : yellow / 배경색 : blue
		// ㅠㅠㅠㅠㅠ => 글자색 : pink / 배경색 : teal
		
		let arrobj = [
			{color: "green", bgc: "red" },
			{color: "yellow", bgc: "blue" },
			{color: "pink", bgc: "teal" }
			];
		
		$("td").each(function(i, t) {
			$(t).css("color", arrobj[i].color);
			$(t).css("background-color", "red");
		});
		$("td").each(function(i, t) {
			$(t).css("color", "yellow");
			$(t).css("background-color", "blue");
		});
		$("td").each(function(i, t) {
			$(t).css("color", "pink");
			$(t).css("background-color", "teal");
		});
		
	});
</script>
</head>
<body>
	<table>
		<tr>
			<td>ㅋㅋㅋㅋㅋ</td>
		</tr>
		<tr>
			<td>ㅎㅎㅎㅎㅎ</td>
		</tr>
		<tr>
			<td>ㅠㅠㅠㅠㅠ</td>
		</tr>
	</table>
</body>
</html>
package com.beaver.feb061.sns;

public class Member {
	private String id;
	private String pw;
	private String name;
	private String phone;
	private String birth;
	private String photo;
	
	
public Member() {
	// TODO Auto-generated constructor stub
}




public Member(String id, String pw, String name, String phone, String birth, String photo) {
	super();
	this.id = id;
	this.pw = pw;
	this.name = name;
	this.phone = phone;
	this.birth = birth;
	this.photo = photo;
}




public String getId() {
	return id;
}


public void setId(String id) {
	this.id = id;
}


public String getPw() {
	return pw;
}


public void setPw(String pw) {
	this.pw = pw;
}


public String getName() {
	return name;
}


public void setName(String name) {
	this.name = name;
}


public String getPhone() {
	return phone;
}


public void setPhone(String phone) {
	this.phone = phone;
}


public String getBirth() {
	return birth;
}


public void setBirth(String birth) {
	this.birth = birth;
}


public String getPhoto() {
	return photo;
}


public void setPhoto(String photo) {
	this.photo = photo;
}


	
	
	
}
